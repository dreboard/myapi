{
    "require": {
        "slim/slim": "^3.7",
        "illuminate/database": "~5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "6.1"
    },
    "autoload": {
        "psr-4": {
            "App\\Controllers\\": "app/controllers/",
            "App\\Services\\": "app/services/",
            "App\\Models\\": [
                "app/models/orm",
                "app/models/dao"
            ],
            "System\\": "system/",
            "APITesting\\": "tests/"
        },
        "autoload-dev": {
            "psr-4": {"APITesting\\": "tests/"}
        },
        "files": ["config/environment.php", "config/constants.php", "config/config.php", "config/functions.php", "tests/SlimBase.php"]
    }

}
