{
    "require": {
        "slim/slim": "^3.7",
        "illuminate/database": "~5.1",
        "monolog/monolog": "^1.23.0"
    },
    "require-dev": {
        "phpunit/phpunit": "6.0.11",
        "phpunit/php-code-coverage": "^5.2",
        "estahn/phpunit-json-assertions": "@stable"
    },
    "autoload": {
        "psr-4": {
            "App\\Controllers\\": "app/controllers/",
            "App\\Services\\": "app/services/",
            "App\\Models\\": [
                "app/models/orm",
                "app/models/dao"
            ],
            "System\\": "system/"
        },
        "autoload-dev": {
            "psr-4": {"APITesting\\": ["tests/", "tests/controllers"]}
        },
        "files": ["config/environment.php", "config/constants.php", "config/config.php", "config/functions.php", "tests/TestBase.php"]
    }

}
